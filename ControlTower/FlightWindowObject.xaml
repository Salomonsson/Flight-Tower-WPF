<Window x:Class="ControlTower.FlightWindowObject"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="FlightWindowObject" Height="376.119" Width="944.477">
    <Grid Margin="53,0,15,25">
        <ListBox x:Name="ListFlightObject" HorizontalAlignment="Left" Height="116" Margin="275,39,0,0" VerticalAlignment="Top" Width="563"/>
        <Image x:Name="AirplaneImg" HorizontalAlignment="Left" Height="116" Margin="22,39,0,0" VerticalAlignment="Top" Width="207" RenderTransformOrigin="0.138,-0.029"/>
        <Button x:Name="START" Content="START" HorizontalAlignment="Left" Margin="326,208,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.403,-1.401" Click="START_Click"/>
        <Button x:Name="LAND" Content="LAND" HorizontalAlignment="Left" Margin="577,209,0,0" VerticalAlignment="Top" Width="75" Click="LAND_Click"/>
        <Button x:Name="btnBackToFlightControl" Content="Tillbaka till Flight Control" HorizontalAlignment="Left" Margin="326,253,0,0" VerticalAlignment="Top" Width="326" RenderTransformOrigin="-0.303,0.622" Click="btnBackToFlightControl_Click"/>
        <ComboBox x:Name="ChangeRouteComboBox" HorizontalAlignment="Left" Margin="433,208,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="ChangeRouteComboBox_SelectionChanged"/>

    </Grid>
</Window>
